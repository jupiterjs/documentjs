    "can": "3.4.1",
    "chokidar": "^1.0.0-rc5",
    "cross-spawn-async": "^2.1.9",
    "documentjs-github-download": "^0.3.0",
    "engine-dependencies": "^0.2.0",
    "esprima": "~3.1.3",
    "fs-extra": "^2.0.0",
    "glob": "~7.1.1",
    "handlebars": "4.0.6",
    "jquery": "~3.1.1",
    "less": "^2.7.2",
    "lodash": "~4.17.4",
